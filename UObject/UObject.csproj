<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <RuntimeIdentifiers>win10-x64;osx-x64;linux-x64</RuntimeIdentifiers>
        <NoWarn>1701;1702;NU1701</NoWarn>
        <Nullable>enable</Nullable>
        <LangVersion>latest</LangVersion>
        <PackageId>UAssetParser</PackageId>
        <Version>1.0.0</Version>
        <Authors>yretenai</Authors>
        <PackageDescription>Parses UObject asset files, mainly for data tables.</PackageDescription>
        <RepositoryUrl>https://github.com/healingbrew/UAssetParser</RepositoryUrl>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="DragonLib" Version="1.1.6" />
    </ItemGroup>
    
</Project>
